<!-- Visit https://www.paypal.com/donate/buttons/ to get your donate button -->
<% const url = get_config_from_obj(service, 'url');
    if (url) { %>
<a class="button is-warning donate" onclick="<%= 'document.getElementById(\'paypal-donate-form\').submit()' %>">
    <span class="icon is-small">
        <i class="fab fa-paypal"></i>
    </span>
    <span><%= __('paypal.me/warisradji') %></span>
</a>
<form action="<%= url %>" method="post" target="_blank" id="paypal-donate-form">
</form>
<% } else { %>
<div class="notification is-danger">
    You forgot to set the <code>url</code> for Paypal. Please set it in <code>_config.yml</code>.
</div>
<% } %>